<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.vignan.artistapp.UI" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ImagesActivity"
            android:label="@string/title_activity_images"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.android.vignan.artistapp.UI.MainActivity" />
        </activity>
        <activity
            android:name=".MusicActivity"
            android:label="@string/title_activity_music"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.android.vignan.artistapp.UI.MainActivity" />
        </activity>
        <activity
            android:name=".VideosActivity"
            android:label="@string/title_activity_videos"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.android.vignan.artistapp.UI.MainActivity" />
        </activity>
        <activity
            android:name=".MailingListActivity"
            android:label="@string/title_activity_mailing_list"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.android.vignan.artistapp.UI.MainActivity" />
        </activity>
        <activity
            android:name=".WallpapersActivity"
            android:label="@string/title_activity_wallpapers"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.android.vignan.artistapp.UI.MainActivity" />
        </activity>
    </application>

</manifest>
